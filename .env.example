# Microsoft Bot Framework credentials
MICROSOFT_APP_ID=
MICROSOFT_APP_PASSWORD=

# Public URL där Teams kan nå din bot (t.ex. https://xyz.ngrok.io)
PUBLIC_BASE_URL=

# Nextcloud OAuth2-konfiguration
NEXTCLOUD_BASE_URL=https://example.nextcloud.com
NEXTCLOUD_OAUTH_CLIENT_ID=
NEXTCLOUD_OAUTH_CLIENT_SECRET=
NEXTCLOUD_OAUTH_REDIRECT_PATH=/auth/callback

# Pollningsintervall i sekunder
NEXTCLOUD_POLL_INTERVAL=60
